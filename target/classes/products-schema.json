{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "description": "The root schema is the schema that comprises the entire JSON document.",
  "default": {},
  "required": [
    "success",
    "code",
    "message",
    "data"
  ],
  "properties": {
    "success": {
      "type": "boolean",
      "enum": [false]
    },
    "message": {
      "type": "string",
      "maxLength": 5
    },
    "code": {
      "type": "integer",
      "multipleOf": 2
    },
    "data": {
      "type": "object"
    }
  }
}